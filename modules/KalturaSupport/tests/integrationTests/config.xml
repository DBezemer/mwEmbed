				<textFormats><!-- ${value} = metadata field,  #[value] = labels --> 
					<textFormat id="mediaInfo"><!-- Info Module: description --> 
						<item><![CDATA[[fontFormat=1]${Series} : ]]></item> 
						<item><![CDATA[[fontFormat=1]${name} <br/><br/>]]></item> 
						<item><![CDATA[[fontFormat=2]${duration|TIME} | ]]></item><!-- |TIME will convert value to time format --> 
						<item><![CDATA[[fontFormat=2]#[SEASON] ${Season}, ]]></item> 
						<item><![CDATA[[fontFormat=2]#[EPISODE] ${Episode} | ]]></item> 
						<item><![CDATA[[fontFormat=2]#[AIRED]: ${Airdate|DATE}<br/>]]></item><!-- |DATE will convert value to date format --> 
						<item><![CDATA[[fontFormat=2]${description}]]></item> 
					</textFormat> 
					<textFormat id="mediaThumbInfo"><!-- Similar Videos Module: description --> 
						<!-- <item><![CDATA[[fontFormat=4]${id} : ]]></item> --> 
						<!-- <item><![CDATA[[fontFormat=4]${name} | ]]></item> --> 
						<item><![CDATA[[fontFormat=4]${description}<br/>]]></item> 
						<item><![CDATA[[fontFormat=4]${duration|TIME}]]></item><!-- |TIME will convert value to time format --> 
						<!-- <item><![CDATA[[fontFormat=4]#[AIRED]: ${startDate|DATE}]]></item> --><!-- |DATE will convert value to date format --> 
					</textFormat> 
					<textFormat id="viewLabel"><!-- Info Module: views --> 
						<item><![CDATA[[fontFormat=3]${views} #[VIEWS]<br/>]]></item> 
					</textFormat> 
					<textFormat id="emailTo"> 
						<item><![CDATA[[fontFormat=5]#[email_to_form]]]></item> 
					</textFormat> 
					<textFormat id="emailFrom"> 
						<item><![CDATA[[fontFormat=5]#[email_from_form]]]></item> 
					</textFormat> 
					<textFormat id="emailBody"> 
						<item><![CDATA[[fontFormat=5]#[email_body_form]]]></item> 
					</textFormat> 
					<textFormat id="emailConfirmation"> 
						<item><![CDATA[[fontFormat=5]#[email_confirmation]]]></item> 
					</textFormat> 
					<textFormat id="closedCaptions"> 
						<item><![CDATA[[fontFormat=2]#[closed_caption]]]></item> 
					</textFormat> 
					<textFormat id="autoPlay"> 
						<item><![CDATA[[fontFormat=2]#[auto_play]]]></item> 
					</textFormat> 
		  		</textFormats> 
				<fontFormats> 
					<fontFormat id="1"><![CDATA[<font color='#ffffff' size='16'>]]></fontFormat> 
					<fontFormat id="2"><![CDATA[<font color='#cccccc' size='12'>]]></fontFormat> 
					<fontFormat id="3"><![CDATA[<font color='#FFFFFF' size='13'>]]></fontFormat> 
					<fontFormat id="4"><![CDATA[<font color='#cccccc' size='12'>]]></fontFormat> 
					<fontFormat id="5"><![CDATA[<font color='#999999' size='10'>]]></fontFormat> 
				</fontFormats> 
			</templates> 
			<conversionTemplates><!-- module will be converted to dictionary with name/value pair --> 
				<conversionTemplate	name="mediaInfo" description="mediaInfo" views="viewLabel" eventType="REQUEST_MEDIA_INFO"/> 
				<conversionTemplate name="similarVideos" description="mediaThumbInfo" eventType="REQUEST_SIMILAR_MEDIA"/> 
			</conversionTemplates> 
			<modulesConfig><!-- module specific configs, these config settings will be passed into each module during a module's registration --> 
				<module	name="FDMShare"> 
					<shareStates> 
						<shareState name="FDMShareSocial" defaultView="true" buttonLabel="share" titleLabel="share"> 
							<config> 
								<site name="Facebook" 	 	id="facebook"	sortOrder="0"	imgUrl="./assets/img/fbIcon.jpg"></site> 
								<site name="MySpace" 	 	id="myspace"	sortOrder="1"	imgUrl="./assets/img/mySpaceIcon.jpg"></site> 
								<site name="Twitter" 		id="twitter" 	sortOrder="2"	imgUrl="./assets/img/twitterIcon.jpg"></site> 
								<site name="Yahoo! Buzz" 	id="yahoo" 		sortOrder="3"	imgUrl="./assets/img/yahooIcon.jpg"></site> 
								<site name="Google Buzz" 	id="google" 	sortOrder="4"	imgUrl="./assets/img/googleIcon.jpg"></site> 
								<site name="Other" 			id="other" 		sortOrder="5"	imgUrl="./assets/img/otherIcon.jpg"></site> 
							</config> 
						</shareState> 
						<shareState name="FDMShareEmbed" defaultView="false" buttonLabel="embed"  titleLabel="embed"> 
							<config><![CDATA[<object classid="clsid:D27CDB6E-AE6D-11cf-96B8-444553540000" codebase="http://download.macromedia.com/pub/shockwave/cabs/flash/swflash.cab#version=6,0,40,0" width="${width}" height="${height}"><param name="movie" value="${swf}"><param name="quality" value="high"><param name="bgcolor" value="#FFFFFF"><!--[if !IE]> <--><object data="${swf}" width="${width}" height="${height}" type="application/x-shockwave-flash"><param name="quality" value="high"><param name="bgcolor" value="#FFFFFF"><param name="pluginurl" value="http://www.adobe.com/go/getflashplayer"></object><!--> <![endif]--></object>]]></config> 
						</shareState> 
						<shareState name="FDMShareEmail" defaultView="false" buttonLabel="email"  titleLabel="email"> 
							<config> 
								<conversionTemplate name="email" to="emailTo" from="emailFrom" body="emailBody" confirmation="emailConfirmation" /> 
							</config> 
						</shareState> 
						<shareState name="FDMShareLink" defaultView="false" buttonLabel="link"  titleLabel="link"> 
							<config></config> 
						</shareState> 
					</shareStates> 
				</module> 
				<module	name="FDMSettings"> 
					<conversionTemplate name="labels" cc="closedCaptions" autoPlay="autoPlay" /> 
				</module> 
			</modulesConfig> 
			<!-- FDMInfo,FDMSimilarVideos,FDMSettings --> 
	  	</FDMConfig> 
