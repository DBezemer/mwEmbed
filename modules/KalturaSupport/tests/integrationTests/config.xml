    	<socialConfig> 
			<socialSite name="facebook"> 
				<script><![CDATA[
					function(){
						var rand = Math.floor(Math.random()*99999);
						var VAMP_social_iframe = document.createElement("IFRAME");
						if(VAMP_social_iframe.style.setAttribute){
							VAMP_social_iframe.style.setAttribute("height", "1px");
							VAMP_social_iframe.style.setAttribute("width", "1px");
							VAMP_social_iframe.style.setAttribute("position", "absolute");
							VAMP_social_iframe.style.setAttribute("top", "-99999px");
							VAMP_social_iframe.style.setAttribute("left", "-99999px");
						}else{
							VAMP_social_iframe.setAttribute("style", "height:1px;width:1px;position:absolute;top:-999px;left:-999px;z-index:-1");
						}
 
						VAMP_social_iframe.setAttribute("id","VAMP_social_fb" + rand);
						VAMP_social_iframe_src = 'http://vampdev.foxfdm.com/facebook.php?';
						VAMP_social_iframe_src += 'fbName=${title}';
						VAMP_social_iframe_src += '&fbMessage=${message}';
						VAMP_social_iframe_src += '&fbDescription=${description}';
						VAMP_social_iframe_src += '&fbLink=${shareUrl}';
						VAMP_social_iframe_src += '&fbPicture=http://www.healthline.com/blogs/diet_nutrition/uploaded_images/beans,jpg-700668.jpg';
						VAMP_social_iframe_src += '&parentLocation=' + window.location.href;
 
						VAMP_social_iframe.setAttribute("src",VAMP_social_iframe_src);
						document.body.appendChild(VAMP_social_iframe);
 
						var VAMP_social_interval = self.setInterval(
							function(){
								var VAMP_social_fb = document.getElementById("VAMP_social_fb" + rand);
								if(window.location.hash == "#success"){
								//alert("Post was published.");
								//Call to the player function goes in here - success;
								window.clearInterval(VAMP_social_interval);
								VAMP_social_fb.parentNode.removeChild(VAMP_social_fb); 
							}
							else if (window.location.hash == "#failure"){
								//alert("Post was not published.");
								//Call to the player function goes in here - failure;
								window.clearInterval(VAMP_social_interval);
								VAMP_social_fb.parentNode.removeChild(VAMP_social_fb); 
							}
							window.location.hash = '';
 
						}, 1000);
					}
				]]></script> 
			</socialSite> 
